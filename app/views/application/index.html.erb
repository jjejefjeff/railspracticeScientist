<center><h1> List of scientists </h1></center>
<% @scientists.each do |scientist|%>
  <div class="stripe">
    <div class="container">
      <div class="row">
        <div class="col-md-4 col-md-offset-4">
          <%= scientist.name  %> - <a href="/profile/<%= scientist.id%>">show </a> | <a href="/profile/<%= scientist.id%>/edit"> edit </a> | <a href="profile/<%= scientist.id%>/destroy">destroy</a>
        </div>
      </div>
    </div>
</div>
<% end %>